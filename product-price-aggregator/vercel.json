{
    "version": 2,
    "builds": [
        {
            "src": "src/app.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/prices(.*)",
            "dest": "/src/app.ts"
        },
        {
            "src": "/(.*)",
            "dest": "/src/app.ts"
        }
    ]
}
